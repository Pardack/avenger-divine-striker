{"_id":"KCp0gFiPeX16iDy1","name":"Incite Mass Retribution","type":"feat","img":"systems/pf2e/icons/default-icons/feats.webp","data":{"description":{"value":"<p>When you use Incite Retribution, you and all allies gain a +1 status bonus to attack rolls for 1 round against your Object of Enmity.</p>"},"source":{"value":"Avenger - The Divine Striker by Fanatic66"},"traits":{"value":["hb_avenger"],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-04T16:12:07.820-07:00","version":{"schema":null,"foundry":"9.269","system":"3.13.5.16"}}},"level":{"value":10},"featType":{"value":"class"},"onlyLevel1":false,"maxTakable":1,"actionType":{"value":"passive"},"actionCategory":{"value":""},"actions":{"value":null},"prerequisites":{"value":[{"value":"Incite Retribution"},{"value":"Censure of Unity"}]},"location":null},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"U618T5YHQPQrR1QD":3},"flags":{}}
